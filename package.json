{
  "name": "fixed-box",
  "version": "0.0.0",
  "description": "Fixed box component for React",
  "main": "cjs/index.js",
  "module": "es/index.js",
  "unpkg": "dist/index.min.js",
  "jsdelivr": "dist/index.min.js",
  "files": [
    "dist/*",
    "cjs/*",
    "es/*",
    "LICENSE",
    "README.md"
  ],
  "repository": "git@github.com:Eazymov/fixed-box.git",
  "author": {
    "name": "Eduard Troshin",
    "email": "eazymovcode@gmail.com"
  },
  "scripts": {
    "dev": "",
    "test": "",
    "lint": "eslint -c .eslintrc.json src/*",
    "build": "yarn build:cjs && yarn build:es && yarn build:iife",
    "build:cjs": "cross-env NODE_ENV=cjs babel src --out-dir cjs --copy-files",
    "build:es": "cross-env NODE_ENV=es babel src --out-dir es --copy-files",
    "build:iife": "cross-env NODE_ENV=iife rollup -c -o dist/index.min.js"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-react": "^7.10.0",
    "flow-bin": "^0.75.0",
    "husky": "^0.14.3",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
}
